#!/bin/bash
                          # Use Bash Shell

#SBATCH -J SFD          #Job name
#SBATCH -o SFD.%j.out   # stdout; %j expands jobid
#SBATCH -e SFD.%j.err   # stderr; skip to combine stdout and stderr
#SBATCH -p normal        # queue
#SBATCH -N 11              # Number of nodes, not cores (16 cores/node)
#SBATCH -n 264             # Total number of MPI tasks (if omitted, n=N)
#SBATCH -t 00:10:00       # max time

#SBATCH --mail-user=siddarth.chintamani@mavs.uta.edu
#SBATCH --mail-type=ALL

#SBATCH -A Three-Dimensional-Ef

                          # Run the MPI executable named "addexe"
ibrun ./viscousDriver2d.Linux.64.mpicxx.ifort.DEBUG.MPI.ex jet.inputs
